_question":false, "frame_position":2, "typewriter":true, "typewriter_speed":0.05, "connection":"", "face_frame":4, "background_frame":5}, {"face_position":0, "beep_pitch":1, "answers":[], "beep":false, "text":"\"Talvez essa seja A MINHA CHANCE de recomeçar", "enable_question":false, "frame_position":2, "typewriter":true, "typewriter_speed":0.05, "connection":"FINALYOGA", "face_frame":4, "background_frame":5}], "FINALTEATRO":[{"answers":[], "beep_pitch":1, "face_position":0, "beep":false, "text":"Entrar para esse grupo de teatro foi uma ótima ideia", "enable_question":false, "typewriter_speed":0.05, "typewriter":true, "frame_position":2, "connection":"", "face_frame":4, "background_frame":8}, {"answers":[], "beep_pitch":1, "face_position":0, "beep":false, "text":"Hoje consigo me expressar um pouco melhor e lido com várias situações que antigamente eu não conseguia resolver", "enable_question":false, "typewriter_speed":0.05, "typewriter":true, "frame_position":2, "connection":"", "face_frame":4, "background_frame":8}, {"answers":[], "beep_pitch":1, "face_position":0, "beep":false, "text":"Já estou até pensando em tentar pedir ajuda profissional psicológica", "enable_question":false, "typewriter_speed":0.05, "typewriter":true, "frame_position":2, "connection":"", "face_frame":4, "background_frame":8}, {"answers":[], "beep_pitch":1, "face_position":0, "beep":false, "text":"Acho que seria ótimo para desenvolver ainda mais minhas habilidades sociais", "enable_question":false, "typewriter_speed":0.05, "typewriter":true, "frame_position":2, "connection":"", "face_frame":4, "background_frame":8}, {"answers":[], "beep_pitch":1, "face_position":0, "beep":false, "text":"Teatro em conjunto com um psicólogo será perfeito!", "enable_question":false, "typewriter_speed":0.05, "typewriter":true, "frame_position":2, "connection":"", "face_frame":4, "background_frame":8}], "D1NOITETrilha01OP01":[{"answers":[], "beep_pitch":1, "face_position":1, "beep":false, "text":"Desculpa por hoje m